    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-native-web": "^0.21.2",
    "tamagui": "1.138.0",
    "webextension-polyfill": "catalog:"
  },
  "devDependencies": {
    "@tanstack/router-cli": "^1.136.6",
    "@tanstack/router-plugin": "^1.136.5",
    "@types/jsdom": "27.0.0",
